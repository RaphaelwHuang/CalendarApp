<p id="notice"><%= notice %></p>

<h1>Listing Comprises</h1>

<table>
  <thead>
    <tr>
      <th>Group</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comprises.each do |comprise| %>
      <tr>
        <td><%= comprise.group %></td>
        <td><%= comprise.user %></td>
        <td><%= link_to 'Show', comprise %></td>
        <td><%= link_to 'Edit', edit_comprise_path(comprise) %></td>
        <td><%= link_to 'Destroy', comprise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comprise', new_comprise_path %>
